{
  "name": "solana-mm-bot",
  "version": "1.0.0",
  "description": "Telegram bot for Solana MM",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "debug": "nodemon --exec ts-node --inspect-brk src/index.ts",
    "serve": "node dist/index.js",
    "start": "nodemon src/index.ts --exec ts-node",
    "test": "jest --runInBand --verbose",
    "coverage": "jest --coverage --runInBand --verbose",
    "eslint": "eslint src/**/*.ts --ignore-pattern \"node_modules/\"",
    "lint": "eslint src --fix",
    "release": "npm version patch && git push && git push --tags"
  },
  "keywords": [
    "telegram",
    "telegraf",
    "starter",
    "template",
    "bot"
  ],
  "author": {
    "name": "Shivank Kapur",
    "email": "shivankkapur2004@gmail.com"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.3",
    "@babel/node": "^7.19.1",
    "@babel/preset-env": "^7.19.4",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.19.86",
    "@types/sequelize": "^4.28.20",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.25.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.2.0",
    "jest-cli": "^29.2.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "ts-jest": "^29.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "sequelize": "^6.31.0",
    "telegraf": "^4.10.0"
  }
}
